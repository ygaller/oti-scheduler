{
  "name": "scheduling-server",
  "version": "1.0.16",
  "description": "Backend API server for scheduling application",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "NODE_ENV=test jest --ci --coverage --watchAll=false",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "ts-node src/database/seed.ts",
    "db:test:setup": "NODE_ENV=test npx prisma migrate deploy",
    "db:test:reset": "NODE_ENV=test npx prisma migrate reset --force"
  },
  "dependencies": {
    "@prisma/client": "^5.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "google-auth-library": "^10.2.1",
    "googleapis": "^156.0.0",
    "pg": "^8.11.3",
    "portfinder": "^1.0.32",
    "postgres": "^3.4.4",
    "prisma": "^5.8.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.10.6",
    "@types/pg": "^8.10.9",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.7",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3",
    "uuid": "^9.0.1"
  },
  "keywords": [
    "scheduling",
    "api",
    "postgresql"
  ],
  "author": "",
  "license": "MIT"
}
